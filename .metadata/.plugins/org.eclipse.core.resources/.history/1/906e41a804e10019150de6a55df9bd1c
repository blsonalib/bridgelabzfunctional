package javaprograms;

import java.util.Scanner;

public class HarmonicNumber 
{

	public static void main(String[] args) 
	{
		double val=0.0;
		
		
		int n=Integer.parseInt(args[0]);
				System.out.println("Enter the number:");
		for(int i=1;i<=n;i++)
		{
			val=val+(double)1/i;
			System.out.println(+val);
		}
	}

}
