package com.bridgelabz.algorithm;

public class InsertionSortInteger 
{

	public static void main(String[] args) 
	{
		int a[]= {2,5,4,7,8};
		int i,j;
		int temp;
		for( i=1;i<a.length;i++)
		{
			temp=a[i];
			j=i;
			while(j>0&&a[j-1]>temp)
			{
				a[j]=a[j-i];
				j=j-1;
			}
			a[j]=temp;
			
		}
		for(i=0;i<a.length;i++)
		{
			System.out.println("array"+a[i]);
		}
	}

}
