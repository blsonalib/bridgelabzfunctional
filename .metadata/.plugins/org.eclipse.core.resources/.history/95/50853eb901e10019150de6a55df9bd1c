package javaprograms;

import java.util.Scanner;

public class HarmonicNumber 
{

	public static void main(String[] args) 
	{
		double n,val;
		Scanner s=new Scanner(System.in);
		System.out.println("Enter the number:");
		n=s.nextDouble();
		for(int i=1;i<=n;i++)
		{
			val=n+(1/n);
			System.out.println(+val);
		}
	}

}
