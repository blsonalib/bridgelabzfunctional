package com.bridgelabz.utility;

import java.util.Scanner;

public class UtilityD 
{
	public static Scanner scanner=new Scanner(System.in);
	public static String getString()
	{
		return  scanner.next();
	}
	
	
	public static int[][] isPrime(int array[][],int num,int j)
	
	{
		
		int k = 0;	
		for(int i = 0; i < array.length; i++)
		{
			for(j = 0; j < array[i].length; j++)
			{
				if(array[i][j] != 0)
				System.out.print("\t " +array[i][j]);
			}
			System.out.println();
		}	
		while(num < 1000)
		{
			boolean st = true;
			if(num == 1 || num == 2)
			{
				num ++;
				continue;
			}
			for(int i = 2; i < num  ; i++)
			{
				if(num % i == 0)
				{   
					st = false;
					break;	
				}	
			}	
			if(st) 
			{
				k = num / 100;
				j = num % 100;
				array[k][j] = num;
			}
			num ++;
		}
		return array;

}
}