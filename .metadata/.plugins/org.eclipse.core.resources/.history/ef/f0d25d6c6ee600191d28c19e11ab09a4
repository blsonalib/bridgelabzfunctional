package com.bridgelabz.algorithm;

import com.bridgelabz.utility.UtilityAlgorithm;

public class SwappingBinary 
{

	public static void main(String[] args)
	{
		int number=0;
		int newNumber;
		System.out.println("Before swappin:");
		newNumber=UtilityAlgorithm.getInteger();
		System.out.println("After swapping:");
		
		System.out.println("Before swappin:"+number);
		System.out.println("After swapping:"+newNumber);
		UtilityAlgorithm.SwappingBinary(number, newNumber);		
		
	}

}
