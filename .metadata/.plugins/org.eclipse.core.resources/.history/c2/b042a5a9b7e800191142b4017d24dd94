package com.bridgelabz.algorithm;

public class InsertionSortInteger 
{

	public static void main(String[] args) 
	{
		int a[]= {2,5,4,7,8};
		int i=1,j;
		int temp;
		for( i=1;i<a.length;i++)
		{
			temp=a[i];
			j=i;
			while(j>0&&a[j-1]>a[j])
			{
				a[j]=a[j-1];
			}
			a[i]=temp;
			j=j-1;
		}
		for(i=1;i<a.length;i++)
		{
			System.out.println("after sorting:"+a[i]);
		}
	}

}
