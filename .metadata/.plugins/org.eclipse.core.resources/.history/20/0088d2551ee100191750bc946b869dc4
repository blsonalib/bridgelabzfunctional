package com.bridgelabz.utility;

public class Utility {
	/**
	 * @param number
	 */
	public static void primeFactor(int number)
	{
		for(int i=2;i<=number;i++)
	    {
	    	while(number%i==0)
	    	{
	    		number=number/i;
	    		System.out.println(+i);
	    	}
	    	if(number<1)
	    	System.out.println(+number);
	    }
	}

}
