package javaprograms;

import java.util.Scanner;

public class HarmonicNumber 
{

	public static void main(String[] args) 
	{
		double n,val=0.0;
		
		System.out.println("Enter the number:");
		n=Integer.parseInt("nL");
		for(int i=1;i<=n;i++)
		{
			val=val+(double)1/i;
			System.out.println(+val);
		}
	}

}
