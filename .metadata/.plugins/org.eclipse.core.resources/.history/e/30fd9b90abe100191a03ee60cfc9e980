package com.bridgelabz.functional;

public class EuclideanDistance 
{

	public static void main(String[] args) 
	{
		int x=2;
		int y=3;
		double sum1,sum2,distance;
		sum1=Math.pow(x, 2);
		sum2=Math.pow(y, 2);
		distance=sum1+sum2;
		System.out.println("Distance from("+x+","+y+")to(0,0)="+distance);
					
	}

}
