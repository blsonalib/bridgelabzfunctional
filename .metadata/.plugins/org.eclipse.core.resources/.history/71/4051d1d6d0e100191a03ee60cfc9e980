package com.bridgelabz.functional;

import java.util.concurrent.TimeUnit;

import com.bridgelabz.utility.Utility;




public class StopWatch 
{

	public static void main(String[] args) throws InterruptedException 
	{   
		long start=0,stop=0,elapsedTime=0;
		int n=5;
		
		System.out.println("Elapsed time of stopwatch:"+elapsedTime);
		System.out.println("Elapsed time of stopwatch in millisecond:"+elapsedTime/1000);
		Utility.StopWatch(start, stop, elapsedTime, n);
	}
}