package com.bridgelabz.algorithm;

public class BinaryWord 
{

	public static void main(String[] args) 
	{
		String stringList[]= {"amit","bhoomi","janvi","siya","tiya"};
		String stringFind="siya";
		int lowest=0;
		int highest=stringList.length-1;
		int middle=0;
		while(lowest<=highest)
		{
			middle=(lowest+highest)/2;
			{
			if(stringFind.compareTo(stringList[middle])>0)
			{
				highest=middle+1;
			}
			else if(stringFind.compareTo(stringList[middle])<0)
			{
				highest=middle-1;
			}
			else
			{
				break;
			}
			}
		}
			if(lowest>highest)
			{
				System.out.println("String not found");
			}
			else
			{
				System.out.println("String found at"+middle);
			}
	   }

    }

