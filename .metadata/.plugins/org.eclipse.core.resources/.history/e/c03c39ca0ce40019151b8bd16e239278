package com.bridgelabz.algorithm;

import com.bridgelabz.utility.UtilityAlgorithm;

public class TemperatureConversion 
{
	static double Conversion(double fahrenheit)
	{
		return ((fahrenheit-32)*5/9);
		
	}
	
	public static void main(String[] args)
	{
	double celsius;
		double temperature=UtilityAlgorithm.getFloat();
		System.out.println("Enter the temperatur:");
		
		celsius=TemperatureConversion.Conversion(66.999); 
		System.out.println("Temperature in celcius:"+celsius);
	}

}
