package com.bridgelabz.algorithm;

import com.bridgelabz.utility.UtilityAlgorithm;

public class TemperatureConversion 
{
	static float Conversion(float fahrenheit)
	{
		return (fahrenheit);
		
	}
	
	public static void main(String[] args)
	{
		float celsius;
		float fahrenheit=UtilityAlgorithm.getFloat();
		System.out.println("Enter the temperatur:");
		celsius=(fahrenheit-32)*5/9;
		System.out.println("Temperature in celcius:"+celsius);
	}

}
